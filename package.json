{
  "name": "@vaultrice/react-components",
  "globalName": "VaultriceReactComponents",
  "version": "0.9.14",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./cjs": {
      "types": "./types/index.d.ts",
      "default": "./dist/cjs/index.js"
    },
    "./esm": {
      "types": "./types/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && rollup -c && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json && rm -rf types && tsc -d --declarationDir types --declarationMap --emitDeclarationOnly",
    "lint": "eslint .",
    "test": "npm run lint",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --disable-telemetry --output-dir docs && echo 'prevent gh from using jekyll' > docs/.nojekyll",
    "start": "npm run storybook",
    "docs": "typedoc",
    "serve-docs": "npm run docs && http-server docs -o",
    "serve-storybook": "npm run build-storybook && http-server docs -o",
    "preversion": "npm run test && npm run build",
    "postversion": "npm run build-storybook && git add . && git commit -a -m 'docs update' && git push && git push --tags"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.4",
    "@storybook/addon-docs": "^9.1.4",
    "@storybook/addon-onboarding": "9.1.4",
    "@storybook/react-vite": "^9.1.4",
    "@types/node": "24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "9.35.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.4",
    "globals": "^16.3.0",
    "http-server": "14.1.1",
    "neostandard": "0.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup": "4.50.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-typescript2": "0.36.0",
    "storybook": "^9.1.4",
    "typedoc": "0.28.12",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@vaultrice/react": "0.9.8"
  },
  "keywords": [
    "vaultrice",
    "storage",
    "encryption",
    "real-time",
    "key-value",
    "persistent",
    "websocket",
    "react",
    "UI",
    "components"
  ],
  "author": "The Vaultrice Team <support@vaultrice.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vaultrice/react-components.git"
  },
  "homepage": "https://www.vaultrice.com",
  "bugs": {
    "url": "https://github.com/vaultrice/react-components/issues"
  }
}
